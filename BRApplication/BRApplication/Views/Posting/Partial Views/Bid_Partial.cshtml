@model BRApplication.Models.MarketPost

<div class="BidLine">
    <table>
       @for(int i = 0; i < Model.Bids.Count(); i++) {
           <tr>
             <td>@Model.Bids.ElementAt(i).Buyer</td>
             <td>@Model.Bids.ElementAt(i).Seller</td>
             <td>$@Model.Bids.ElementAt(i).BidPrice</td>
           </tr>
       }
    </table>
</div>